<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bpp.projects.cardGuide.dao.UserDAO">
    <insert id="insertUser" useGeneratedKeys="true" keyProperty="id">
       INSERT INTO usersTable(nickName,avatarUrl,country,province,city,gender,`language`,createTime,openid,lastVisitTime)
       VALUES (#{nickName},#{avatarUrl},#{country},#{province},#{city},#{gender},#{language},#{createTime},#{openid},#{lastVisitTime})
    </insert>
</mapper>